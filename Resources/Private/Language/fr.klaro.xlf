    <file source-language="en" target-language="fr" date="2020-02-03T12:57:09+01:00">
        <body>
            <trans-unit id="consentModal.title">
                <source>Information that we collect</source>
                <target>Les informations que nous collectons</target>
            </trans-unit>
            <trans-unit id="consentModal.description">
                <source>Here you can see and customize the information that we collect about you.</source>
                <target>Ici, vous pouvez voir et personnaliser les informations que nous collectons sur vous.</target>
            </trans-unit>
            <trans-unit id="consentModal.privacyPolicy.name">
                <source>privacy policy</source>
                <target>politique de confidentialité</target>
            </trans-unit>
            <trans-unit id="consentModal.privacyPolicy.text">
                <source>To learn more, please read our {privacyPolicy}.</source>
                <target>Pour en savoir plus, merci de lire notre {privacyPolicy}.</target>
            </trans-unit>
            <trans-unit id="ok">
                <source>OK</source>
                <target>OK</target>
            </trans-unit>
            <trans-unit id="close">
                <source>Save</source>
                <target>Fermer</target>
            </trans-unit>
            <trans-unit id="decline">
                <source>Decline</source>
                <target>Refuser</target>
            </trans-unit>
            <trans-unit id="save">
                <source>Save</source>
                <target>Sauvegarder</target>
            </trans-unit>
            <trans-unit id="consentNotice.description">
                <source>We collect and process your personal information for the following purposes: {purposes}.</source>
                <target>Nous collectons et traitons vos informations personnelles dans le but suivant : {purposes}.</target>
            </trans-unit>
            <trans-unit id="consentNotice.changeDescription">
                <source>There were changes since your last visit, please update your consent.</source>
                <target>Des modifications ont eu lieu depuis votre dernière visite, merci de mettre à jour votre consentement.</target>
            </trans-unit>
            <trans-unit id="consentNotice.learnMore">
                <source>Learn more</source>
                <target>En savoir plus</target>
            </trans-unit>
            <trans-unit id="service.disableAll.title">
                <source>Toggle all apps</source>
                <target>Changer toutes les options</target>
            </trans-unit>
            <trans-unit id="service.disableAll.description">
                <source>Use this switch to enable/disable all apps.</source>
                <target>Utiliser ce bouton pour activer/désactiver toutes les options</target>
            </trans-unit>
            <trans-unit id="service.optOut.title">
                <source>(opt-out)</source>
                <target>(opt-out)</target>
            </trans-unit>
            <trans-unit id="service.optOut.description">
                <source>This app is loaded by default (but you can opt out)</source>
                <target>Cette application est chargée par défaut (mais vous pouvez la désactiver)</target>
            </trans-unit>
            <trans-unit id="service.required.title">
                <source>(always required)</source>
                <target>(toujours requis)</target>
            </trans-unit>
            <trans-unit id="service.required.description">
                <source>This application is always required</source>
                <target>Cette application est toujours requise</target>
            </trans-unit>
            <trans-unit id="service.purposes">
                <source>Purposes</source>
                <target>Utilisations</target>
            </trans-unit>
            <trans-unit id="service.purpose">
                <source>Purpose</source>
                <target>Utilisation</target>
            </trans-unit>
        </body>
