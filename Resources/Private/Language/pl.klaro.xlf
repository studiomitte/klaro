                <target>Gromadzenie statystyk odwiedzin, aktywacja JavaScript</target>
            </trans-unit>
            <trans-unit id="googleAnalytics.description">
                <source>Collection of visitor statistics</source>
                <target>Gromadzenie i analiza statystyk odwiedzin</target>
            </trans-unit>
            <trans-unit id="purposes.analytics">
                <source>Analytics</source>
                <target>Gromadzenie i analiza statystyk odwiedzin</target>
            </trans-unit>
