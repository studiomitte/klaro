    <file source-language="en" target-language="ro" date="2020-02-03T12:57:09+01:00">
        <body>
            <trans-unit id="consentModal.title">
                <source>Information that we collect</source>
                <target>Informatii pe care le culegem</target>
            </trans-unit>
            <trans-unit id="consentModal.description">
                <source>Here you can see and customize the information that we collect about you.</source>
                <target>Aici puteti vedea si ajusta ce informatii colectam despre dumneavoastra.</target>
            </trans-unit>
            <trans-unit id="consentModal.privacyPolicy.name">
                <source>privacy policy</source>
                <target>Politica pentru protectia datelor</target>
            </trans-unit>
            <trans-unit id="consentModal.privacyPolicy.text">
                <source>To learn more, please read our {privacyPolicy}.</source>
                <target>Va rugam sa cititi {privacyPolicy} pentru mai multe detalii.</target>
            </trans-unit>
            <trans-unit id="ok">
                <source>OK</source>
                <target>De acord</target>
            </trans-unit>
            <trans-unit id="close">
                <source>Save</source>
                <target>Inchide</target>
            </trans-unit>
            <trans-unit id="decline">
                <source>Decline</source>
                <target>Refuz</target>
            </trans-unit>
            <trans-unit id="save">
                <source>Save</source>
                <target>Salveaza</target>
            </trans-unit>
            <trans-unit id="googleTagManager.description">
                <source>Embed additional JavaScript files</source>
                <target>Integrarea de JavaScript suplimentar</target>
            </trans-unit>
            <trans-unit id="googleAnalytics.description">
                <source>Collection of visitor statistics</source>
                <target>Colectarea de statistici ale vizitatorilor</target>
            </trans-unit>
            <trans-unit id="purposes.analytics">
                <source>Analytics</source>
                <target>Colectarea de statistici ale vizitatorilor</target>
            </trans-unit>
            <trans-unit id="purposes.security">
                <source>Security</source>
                <target>Securitate</target>
            </trans-unit>
            <trans-unit id="consentNotice.description">
                <source>We collect and process your personal information for the following purposes: {purposes}.</source>
                <target>Stocam si prelucram informatiile dumneavoastra personale in urmatoarele scopuri: {purposes}.</target>
            </trans-unit>
            <trans-unit id="consentNotice.changeDescription">
                <source>There were changes since your last visit, please update your consent.</source>
                <target>Au existat modificari de la ultima vizita, va rog sa actualizati consimtamantul dumneavoastra.</target>
            </trans-unit>
            <trans-unit id="consentNotice.learnMore">
                <source>Learn more</source>
                <target>Aflati mai multe</target>
            </trans-unit>
            <trans-unit id="service.disableAll.title">
                <source>Toggle all apps</source>
                <target>Activati/dezactivati toate aplicatiile</target>
            </trans-unit>
            <trans-unit id="service.disableAll.description">
                <source>Use this switch to enable/disable all apps.</source>
                <target>Utilizati acest buton pentru a activa/dezactiva toate aplicatiile.</target>
            </trans-unit>
            <trans-unit id="service.optOut.title">
                <source>(opt-out)</source>
                <target>(Optiune de renuntare)</target>
            </trans-unit>
            <trans-unit id="service.optOut.description">
                <source>This app is loaded by default (but you can opt out)</source>
                <target>Aceasta aplicatie este incarcata implicit (dar puteti renunta)</target>
            </trans-unit>
            <trans-unit id="service.required.title">
                <source>(always required)</source>
                <target>(obligatoriu intotdeauna)</target>
            </trans-unit>
            <trans-unit id="service.required.description">
                <source>This application is always required</source>
                <target>Aceasta aplicatie este necesara intotdeauna.</target>
            </trans-unit>
            <trans-unit id="service.purposes">
                <source>Purposes</source>
                <target>Scopuri</target>
            </trans-unit>
            <trans-unit id="service.purpose">
                <source>Purpose</source>
                <target>Scop</target>
            </trans-unit>
        </body>
