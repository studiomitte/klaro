            <trans-unit id="consentModal.title">
                <source>Information that we collect</source>
                <target>Informácie, ktoré zbierame</target>
            </trans-unit>
            <trans-unit id="consentModal.description">
                <source>Here you can see and customize the information that we collect about you.</source>
                <target>Tu môžete vidieť a prispôsobiť si, ktoré informácie budú Vašim prezeraním stránky zaznamenávané.</target>
            </trans-unit>
            <trans-unit id="consentModal.privacyPolicy.name">
                <source>privacy policy</source>
                <target>Vyhlásenie o ochrane údajov</target>
            </trans-unit>
            <trans-unit id="consentModal.privacyPolicy.text">
                <source>To learn more, please read our {privacyPolicy}.</source>
                <target>Pre viac podrobností si prosím prečítajte si našu {privacyPolicy}.</target>
            </trans-unit>
            <trans-unit id="ok">
                <source>OK</source>
                <target>Akceptovať!</target>
            </trans-unit>
            <trans-unit id="close">
                <source>Save</source>
                <target>Zatvoriť</target>
            </trans-unit>
            <trans-unit id="decline">
                <source>Decline</source>
                <target>Odmietnuť</target>
            </trans-unit>
            <trans-unit id="save">
                <source>Save</source>
                <target>Uložiť</target>
            </trans-unit>
