                <source>Embed additional JavaScript files</source>
                <target>pripojenie dodatočného JavaScriptu</target>
            </trans-unit>
            <trans-unit id="googleAnalytics.description">
                <source>Collection of visitor statistics</source>
                <target>návštevnosť stránky</target>
            </trans-unit>
            <trans-unit id="purposes.analytics">
                <source>Analytics</source>
                <target>návštevnosť stránky</target>
            </trans-unit>
            <trans-unit id="purposes.security">
                <source>Security</source>
                <target>Bezpečnosť</target>
            </trans-unit>
            <trans-unit id="consentNotice.description">
                <source>We collect and process your personal information for the following purposes: {purposes}.</source>
                <target>Ukladáme a spracovávame Vaše osobné informácie pre nasledovné účely: {purposes}.</target>
            </trans-unit>
            <trans-unit id="consentNotice.changeDescription">
                <source>There were changes since your last visit, please update your consent.</source>
                <target>Od Vašej poslednej návštevy došlo k zmenám, prosím aktualizujte svoju voľbu.</target>
            </trans-unit>
            <trans-unit id="consentNotice.learnMore">
                <source>Learn more</source>
                <target>Dozvedieť sa viac</target>
            </trans-unit>
            <trans-unit id="service.disableAll.title">
                <source>Toggle all apps</source>
                <target>Všetky aplikácie zapnúť/vypnúť</target>
            </trans-unit>
            <trans-unit id="service.disableAll.description">
                <source>Use this switch to enable/disable all apps.</source>
                <target>Použite tento prepínač na aktiváciu/deaktiváciu všetkých aplikácií.</target>
            </trans-unit>
            <trans-unit id="service.optOut.title">
                <source>(opt-out)</source>
                <target>(opt-Out)</target>
            </trans-unit>
            <trans-unit id="service.optOut.description">
                <source>This app is loaded by default (but you can opt out)</source>
                <target>Táto aplikácia je štandardne načítaná (ale môžete ju deaktivovať)</target>
            </trans-unit>
            <trans-unit id="service.required.title">
                <source>(always required)</source>
                <target>(vždy potrebné)</target>
            </trans-unit>
            <trans-unit id="service.required.description">
